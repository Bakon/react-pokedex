{
  "name": "pokedex",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://julicolo.github.io/react-pokedex",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "install": "^0.13.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.12.0",
    "npm": "^6.14.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "dev": "run-p start runApi",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "runApi": "node api.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ]
  },
  "prettier": {
    "autoformat": true,
    "extensions": [
      "js",
      "jsx"
    ],
    "printWidth": 100,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": false,
    "editor.formatOnSave": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-prettier": "^3.1.0"
  }
}
